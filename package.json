{
	"name": "consumptions",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "turbo run db:start & sleep 1 && turbo run dev",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"db:start": "turso dev & sleep 1 && bun run db:studio",
		"db:migrate": "drizzle-kit generate:sqlite && bun run drizzle/migrate.ts",
		"db:push": "drizzle-kit push:sqlite",
		"db:seed": "bun run drizzle/seed.ts",
		"db:studio": "drizzle-kit studio --port 3333"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.43.1",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "8.56.9",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^2.37.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.14",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"turbo": "1.13.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.8",
		"vitest": "^1.5.0"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.2",
		"@libsql/client": "^0.6.0",
		"bignumber.js": "^9.1.2",
		"bits-ui": "^0.21.3",
		"clsx": "^2.1.0",
		"drizzle-orm": "^0.30.8",
		"drizzle-zod": "^0.5.1",
		"formsnap": "^1.0.0",
		"mode-watcher": "^0.3.0",
		"nanoid": "^5.0.7",
		"radix-icons-svelte": "^1.2.1",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.22",
		"sveltekit-superforms": "^2.12.4",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"zod": "^3.22.4"
	}
}
