{
	"name": "consumptions",
	"version": "0.0.1",
	"type": "module",
	"packageManager": "bun@1.0.26",
	"private": true,
	"scripts": {
		"build": "vite build",
		"build-cf-types": "wrangler types && mv worker-configuration.d.ts src/",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:migrate": "drizzle-kit generate && bun run drizzle/migrate.ts",
		"db:push": "drizzle-kit push",
		"db:seed": "bun run drizzle/seed.ts",
		"db:studio": "drizzle-kit studio --port 3333",
		"dev": "vite dev",
		"format": "prettier --cache --ignore-unknown --write .",
		"lint": "eslint --fix .",
		"prepare": "husky",
		"preview": "wrangler pages dev .svelte-kit/cloudflare",
		"start": "turbo run db:studio & turbo run dev",
		"test": "npm run test:unit && npm run test:integration",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@internationalized/date": "^3.5.2",
		"@libsql/client": "0.8.0",
		"bignumber.js": "^9.1.2",
		"bits-ui": "^0.21.12",
		"clsx": "^2.1.0",
		"drizzle-orm": "0.32.1",
		"drizzle-zod": "0.5.1",
		"echarts": "5.5.1",
		"formsnap": "^1.0.0",
		"iterator-helpers-polyfill": "3.0.1",
		"mode-watcher": "^0.3.0",
		"nanoid": "5.0.7",
		"pino": "9.3.2",
		"svelte-echarts": "^1.0.0-rc3",
		"svelte-radix": "^1.1.0",
		"svelte-sonner": "^0.3.22",
		"sveltekit-superforms": "^2.12.4",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240405.0",
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "1.43.1",
		"@sveltejs/adapter-cloudflare": "^4.3.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "8.56.9",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "latest",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^2.37.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"pino-pretty": "^11.2.1",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.14",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"turbo": "2.0.11",
		"typescript": "^5.4.5",
		"vite": "^5.2.8",
		"vitest": "2.0.5",
		"wrangler": "^3.50.0"
	},
	"lint-staged": {
		"*.{js,svelte,ts,tsx}": [
			"prettier --cache --write --ignore-unknown",
			"eslint --fix"
		],
		"*.{md,json,toml}": [
			"prettier --cache --write --ignore-unknown"
		]
	}
}
